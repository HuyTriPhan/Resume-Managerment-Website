@model ResumeManagement.Models.BaiDangTuyenDung

@{
    ViewBag.Title = "Xoa";
    Layout = "~/Areas/NhaTuyenDung/Views/Shared/_LayoutNhaTuyenDung.cshtml";
}

<h2>Xóa tin tuyển dụng</h2>

<div>

    <hr />
    <dl class="dl-horizontal" style="margin-left:250px">
        <dt>
            Tiêu đề:
        </dt>
        <dd>
            @Html.DisplayFor(model => model.TieuDe)
        </dd>

        <dt>
            Mô tả
        </dt>
        <dd>
            @Html.DisplayFor(model => model.MoTa)
        </dd>

        <dt>
            Ngày tạo
        </dt>
        <dd>
            @Html.DisplayFor(model => model.NgayTao)
        </dd>

        <dt>
            Số điện thoại
        </dt>
        <dd>
            @Html.DisplayFor(model => model.SoDienThoai)
        </dd>



        <dt>
            Loại công việc
        </dt>
        <dd>
            @Html.DisplayFor(model => model.LoaiCongViec)
        </dd>
        <dt>
            Mã công ty
        </dt>

        <dd>
            @Html.DisplayFor(model => model.MaCongTy)
        </dd>


    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color" style="text-align:center">
            <input type="submit" value="Delete" class="btn btn-default" /> |
            @Html.ActionLink("Quay Về", "DanhSach")
        </div>
    }
</div>